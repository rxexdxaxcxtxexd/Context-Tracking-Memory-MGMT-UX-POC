{
  "hooks": {
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python scripts/session_start_bridge.py",
            "timeout": 10000
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python scripts/session_end_bridge.py",
            "timeout": 30000
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "learning-output-style@claude-code-plugins": true,
    "agent-sdk-dev@claude-code-plugins": true,
    "explanatory-output-style@claude-code-plugins": true,
    "compound-engineering@every-marketplace": true,
    "claude-mem@anthropic": true
  },
  "alwaysThinkingEnabled": true,
  "env": {
    "CLAUDE_MEM_CONTEXT_OBSERVATIONS": "25"
  }
}